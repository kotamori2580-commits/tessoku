#include <bits/stdc++.h>
using namespace std;

int w[109],v[109];
int dp[109][100009];

int main(){
  int N,W;
  cin >> N >> W;
  for(int i=1;i<=n;i++){
    cin >> w[i] >> v[i];
  }
  
  for(int i=0;i<=W;i++){
    dp[0][i] = -100;
  }
  dp[0][0] = 0;
  
  for(int i=1;i<=N;i++){
    for(int j=0;j<=W;j++){
      if(dp[i-1][j] >= 0){
        dp[i][j] = dp[i-1][j];
      }
      else if(j>=w[i] && dp[i][j-w[i]] >= 0){
        dp[i][j] = dp[i][j-w[i]] + v[i];
      }
    }
  }
  
  int ans=0;
  for(int j=0;j<=W;j++){
    if (dp[n][j] > ans){
      ans = dp[n][j];
    }
  }
  
  cout << ans << "\n";
}
