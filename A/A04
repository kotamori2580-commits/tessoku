/**
 * 難易度（体感）：★☆☆☆☆
 * 結果：RE
 * 解法：全探索
 * * 【学んだこと・ポイント】
 * ・
 * * 【反省・詰まった点】
 * ・ループの回数は今回はnではない。そこに注意
 * * 【計算量】
 * ・O(1)
 */

#include <bits/stdc++.h>
using namespace std;

int main(){
  int n;
  cin >> n;
  int m=n;
  string ans= "0000000000";
  for(int i=0;i<n;i++){
    ans[9-i] = m%2 + '0';
    m = m/2;
  }
  cout << ans << "\n";
}
