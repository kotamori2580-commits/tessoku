/**
 * 難易度（体感）：★★★☆☆
 * 結果：できない
 * 解法：優先度付キュー→座標圧縮
 * * 【学んだこと・ポイント】
 * ・v.erase(unique(v.begin(), v.end()), v.end());　でソートして重複を消す
 * * 【反省・詰まった点】
 * ・
 * * 【計算量】
 * ・O()
 */

#include <bits/stdc++.h>
using namespace std;

priority_queue<int, vector<int>, greater<int>> A[100009];
vector<int> B[100009];

int main(){
  int n;
  for (int i=1;i<=n;i++){
    cin >> A[i];
  }
  for (int i=1;i<=n;i++){
    int a;
    a = A.top();
    for (int j=1;j<=n;j++){
      if (A[j] ==a){
        B[j] == i;
      }
      A[j] = 1000000;
    }
  }  
}
