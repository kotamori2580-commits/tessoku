#include <bits/stdc++.h>
using namespace std;

int dp[2009][2009];

int main(){
  string S,T;
  cin >> S >> T;
  for(int i= 1;i<=len(S);i++){
    for(int j=1;j<=len(T);j++){
      if(S[i] == T[j]){
        dp[i][j] = max(dp[i-1][j],dp[i][j-1],dp[i-1][j-1]+1);
      }
      else{
        dp[i][j] = max(dp[i-1][j],dp[i][j-1]);
      }
    }
  }
  cout << dp[len(S][len(T)] << "\n";
}
