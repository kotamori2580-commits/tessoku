#include <bits/stdc++.h>
using namespace std;

long long dp[100][100];
char ls[100][100];

int main(){
  int H, W;
  cin >> H >> W;
  for(int i=1;i<=H;i++){
    for(int j=1;j<=W;j++){
      cin >> ls[i][j];
    }
  }
  
  dp[1][1]=1;
  for(int i=0;i<=H;i++){
    dp[i][0] = 0;
  }
  for(int j=0;j<=W;j++){
    dp[0][j] = 0;
  }
  
  for(int i=1;i<=H;i++){
    for(int j=1;j<=W;j++){
      if(ls[i][j]=='#'){
        dp[i][j]=0;
      }
      else{
        dp[i][j] = dp[i-1][j]+dp[i][j-1];
      }
    }
  }
  cout << dp[H][W] << "\n";
}
