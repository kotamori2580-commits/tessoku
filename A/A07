/**
 * 難易度（体感）：★★★☆☆
 * 結果：TLE
 * 解法：累積和
 * * 【学んだこと・ポイント】
 * ・もっと短くかける
 * * 【反省・詰まった点】
 * ・計算量が多くなりすぎる
 * * 【計算量】
 * ・O(nd)
 */


#include <bits/stdc++.h>
using namespace std;

int main(){
  int d,n;
  cin >> d >> n;
  vector<int> vec(d);
  for(int i=0;i<n;i++){
    int l,r;
    cin >> l >> r;
    for(int j=0;j<d;j++){
      if(j==l-1){
        if(l==1){
          vec[j] ++;
        }
        else{
          vec[j] = vec[j-1]+1;          
        }
      }
      else if(j==r){
        vec[j] = vec[j-1]-1;
      }
      else if(j==0){
        vec[0] = 0;
      }
      else{
        vec[j] = vec[j-1];
      }
    }
  }
  for(int i=0;i<d;i++){
    cout << vec[i] << "\n";
  }
}
